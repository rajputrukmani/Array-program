public class CopyArray {
    public static void main(String[] args) {
        int x[]={10,20,30,40,50};
        int size=5;

        int y[]=new int[size];
        int i,j;
        for(i=0;i<5;i++)
        {
            y[size]=x[i];
        }
        for(j=0;j<5;j++)
        {
            System.out.print(y[size]);
        }
        System.out.println();
    }
    
}
