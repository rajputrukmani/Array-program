 public class StringWhiteSpace {
    public static void main(String[] args) {
        String s1=" Hello";
        String s2="";
        int index=0;
        for(int i=0; i<s1.length(); i++){
            if(i > index){
                s2 += s1.charAt(i);
            }
        }
        System.out.println(s2);
    }
    
}
