 public class LowerCase {
    public static void main(String[] args) {
        String st="HELLO";
        String s2=" ";
        for(int i=0; i<st.length(); i++){
            char ch=st.charAt(i);
            int assivalue=ch;
            if(assivalue >= 64 && assivalue <= 90){
                assivalue += 32;
            }
            s2=s2+(char) assivalue;
        }
        System.out.println(s2);
    }
    
}
